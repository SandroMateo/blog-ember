<form>
  <h3>Please login</h3>
  <div class="form-group">
    <label for="username">Username: </label>
    {{input value=username type="text"}}
  </div>
  <div class="form-group">
    <label for="password">Password: </label>
    {{input value=password type="password"}}
  </div>
  <button class="btn" {{action 'login' users}}>Login</button>
</form>
